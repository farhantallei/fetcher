{
	"name": "@farhantallei/fetcher",
	"version": "1.1.3",
	"license": "MIT",
	"description": "A lightweight, universal HTTP client designed for modern web and Node.js applications. Supports interceptors, authentication, form-data requests, and is fully composable for building scalable and reusable network layers across multiple projects.",
	"keywords": [
		"fetch",
		"interceptor",
		"middleware",
		"request"
	],
	"author": {
		"name": "Farhan Pradana Tallei",
		"email": "farhan.pradana55@gmail.com"
	},
	"scripts": {
		"build": "tsup --config tsup.config.ts",
		"lint": "biome check",
		"format": "biome check --write --unsafe",
		"publint": "publint",
		"typecheck": "tsc --noEmit",
		"test": "bun test",
		"prepare": "bun run build"
	},
	"module": "dist/index.js",
	"type": "module",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"publishConfig": {
		"registry": "https://registry.npmjs.org/",
		"access": "public"
	},
	"homepage": "https://github.com/farhantallei/fetcher",
	"devDependencies": {
		"@biomejs/biome": "2.3.15",
		"@types/bun": "latest",
		"@types/qs": "^6.14.0",
		"publint": "^0.3.17",
		"tsup": "^8.5.1"
	},
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		},
		"./helper": {
			"import": {
				"types": "./dist/helper.d.ts",
				"default": "./dist/helper.js"
			},
			"require": {
				"types": "./dist/helper.d.cts",
				"default": "./dist/helper.cjs"
			}
		},
		"./types": {
			"types": "./dist/types.d.ts"
		}
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"dependencies": {
		"qs": "^6.14.2"
	}
}
